<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student3" class="com.lec6_1.ex.Student">
		<constructor-arg value="홍길동"></constructor-arg>
		<constructor-arg value="8"></constructor-arg>
		<constructor-arg>
			<list>
				<value>헬스</value>
				<value>여행</value>
			</list>
		</constructor-arg>
		
		<property name="height">
			<value>180</value>
		</property>
		
		<property name="weight" value="82"/>
		
	</bean>

	<bean id="family" class="com.lec6_1.ex.Family" c:papaName="홍아빠" c:mamaName="홈엄마" p:sisName="홍누나">
	
		<property name="broName" value= "홍형"></property>
	</bean>


</beans>
